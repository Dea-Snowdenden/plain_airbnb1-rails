<p id="notice"><%= notice %></p>

<h1>Listing Rooms</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Room num</th>
      <th>Bathroom</th>
      <th>Description</th>
      <th>Smoking</th>
      <th>Wifi</th>
      <th>Pets</th>
      <th>Price</th>
      <th>Picture</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @rooms.each do |room| %>
      <tr>
        <td><%= room.name %></td>
        <td><%= room.room_num %></td>
        <td><%= room.bathroom %></td>
        <td><%= room.description %></td>
        <td><%= room.smoking %></td>
        <td><%= room.wifi %></td>
        <td><%= room.pets %></td>
        <td><%= room.price %></td>
        <td><%= room.picture %></td>
        <td><%= link_to 'Show', room %></td>
        <td><%= link_to 'Edit', edit_room_path(room) %></td>
        <td><%= link_to 'Destroy', room, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Room', new_room_path %>
